{
  "version": "1.0.0",
  "name": "react-starter",
  "description": "a simple starter for react",
  "author": "Colin Hill",
  "main": "index.js",
  "private": true,
  "scripts": {
    "build-dev": "webpack --mode development",
    "lint": "eslint ./src --fix",
    "storybook": "start-storybook -p 6006 -c .storybook",
    "test": "jest",
    "test:coverage": "jest --watch --coverage",
    "test:watch": "jest --watch",
    "start": "serve -s dist",
    "dev": "webpack-dev-server --mode development --env.env=dev",
    "dev:bundleanalyze": "npm run dev -- --env.addons=bundleanalyze",
    "build": "webpack --env.env=prod",
    "build:bundleanalyze": "npm run build -- --env.addons=bundleanalyze",
    "build:bundlevisualizer": "npm run build -- --env.addons=bundlevisualizer"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-stage-2": "^7.0.0",
    "@storybook/addon-actions": "^4.1.3",
    "@storybook/addon-jest": "^4.1.3",
    "@storybook/addon-knobs": "^4.1.3",
    "@storybook/addon-links": "^4.1.3",
    "@storybook/react": "^4.1.3",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "babel-register": "^6.26.0",
    "compression-webpack-plugin": "^2.0.0",
    "css-loader": "^2.0.2",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "eslint": "^5.11.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "file-loader": "^3.0.1",
    "ignore-styles": "^5.0.1",
    "jest": "^23.6.0",
    "jest-enzyme": "^7.0.1",
    "jsdom": "^13.1.0",
    "node-sass": "^4.11.0",
    "react-addons-test-utils": "^15.6.2",
    "react-hot-loader": "^4.6.3",
    "react-test-renderer": "^16.7.0",
    "sass-loader": "^7.1.0",
    "sinon": "^7.2.2",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.1.0",
    "url-loader": "^1.1.2",
    "webpack": "^4.28.2",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.13",
    "webpack-merge": "^4.1.5",
    "webpack-visualizer-plugin": "^0.1.11"
  },
  "dependencies": {
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/.jest/setupTests.js",
    "testPathIgnorePatterns": [
      "<rootDir>/__tests__/setup/",
      "<rootDir>/node_modules"
    ],
    "setupFiles": [
      "<rootDir>/.jest/helpers.js",
      "<rootDir>/.jest/dom.js"
    ],
    "testURL": "http://localhost",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/.jest/assetsTransformer.js"
    },
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/.jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/.jest/fileTransform.js"
    }
  }
}
